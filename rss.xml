&lt;/div&gt;
&lt;div id="outline-container-sec-1" class="outline-2"&gt;
&lt;h2 id="sec-1"&gt;&lt;span class="section-number-2"&gt;1&lt;/span&gt; code&lt;/h2&gt;
&lt;div class="outline-text-2" id="text-1"&gt;
&lt;div class="org-src-container"&gt;

&lt;pre class="src src-python"&gt;&lt;span class="org-keyword"&gt;def&lt;/span&gt; &lt;span class="org-function-name"&gt;get_mac_address&lt;/span&gt;():
    &lt;span class="org-keyword"&gt;import&lt;/span&gt; uuid
    &lt;span class="org-variable-name"&gt;node&lt;/span&gt; = uuid.getnode()
    &lt;span class="org-variable-name"&gt;mac&lt;/span&gt; = uuid.UUID(&lt;span class="org-builtin"&gt;int&lt;/span&gt; = node).&lt;span class="org-builtin"&gt;hex&lt;/span&gt;[-12:]
    &lt;span class="org-keyword"&gt;return&lt;/span&gt; mac
&lt;/pre&gt;
&lt;/div&gt;
&lt;/div&gt;
&lt;/div&gt;

